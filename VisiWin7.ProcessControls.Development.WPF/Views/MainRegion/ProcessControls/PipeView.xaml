	    <pc:Pipe Width="174" Height="10" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="836,493,0,0"/>
	    <pc:PipeConnection Width="16" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="912,489,0,0" Height="18"/>
	    <pc:Pipe Width="120" Height="12" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="672,491,0,0"/>
	    <pc:PipeBridge Width="56" Height="41" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="786,466,0,0" StrokeThickness="4" StructVariableName=""/>
	    <pc:Arrow Width="20" Height="42" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="662,476,0,0" Orientation="Left"/>
	    <pc:Arrow Height="47" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="905,359,0,0" Width="31" Orientation="Bottom"/>
    </Grid>

</vw:View>