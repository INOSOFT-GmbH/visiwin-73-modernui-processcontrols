<vw:View x:Class="HMI.ValveView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vw="http://inosoft.com/visiwin7"
    xmlns:pc="http://inosoft.com/ProcessControls"
    mc:Ignorable="d"
    d:DesignWidth="1024" d:DesignHeight="588">

    <Grid x:Name="LayoutRoot" Background="{DynamicResource ApplicationBackgroundBrush}">
	    <pc:Valve Width="133" Height="158" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="39,23,0,0"/>
	    <pc:PistonValve Width="164" Height="182" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="235,35,0,0"/>
	    <pc:MotorValve Width="173" Height="234" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="434,22,0,0" StructVariableName="ValveOne"/>
	    <pc:AngleValve Height="198" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="651,61,0,0" Width="134"/>
	    <pc:ThreeWayValve Width="162" Height="180" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="834,65,0,0"/>
	    <pc:GlobeValve Width="158" Height="160" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="43,231,0,0"/>
	    <pc:FlapValve Width="205" Height="184" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="250,241,0,0"/>
	    <pc:GateValve Width="189" Height="184" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="505,251,0,0"/>
	    <pc:BallValve Width="159" Height="199" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="774,256,0,0"/>
    </Grid> 	
    
</vw:View>