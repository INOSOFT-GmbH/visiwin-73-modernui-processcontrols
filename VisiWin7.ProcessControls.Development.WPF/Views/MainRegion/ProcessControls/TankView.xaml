<vw:View x:Class="HMI.TankView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vw="http://inosoft.com/visiwin7"
    xmlns:pc="http://inosoft.com/ProcessControls"
    mc:Ignorable="d"
    d:DesignWidth="1024" d:DesignHeight="588">

    <Grid x:Name="LayoutRoot" Background="{DynamicResource ApplicationBackgroundBrush}">
        <pc:Tank Width="245" Height="255" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="36,33,0,0" Orientation="Top" StructVariableName="" ActualValue="50"/>
	    <pc:RoundTank Width="179" Height="255" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="285,33,0,0" Orientation="Top" StructVariableName="" ActualValue="50"/>
	    <pc:Silo Width="241" Height="255" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="468,33,0,0" Orientation="Top" StructVariableName="" ActualValue="50"/>
    </Grid> 	
    
</vw:View>